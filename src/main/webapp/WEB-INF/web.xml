<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>pharmInfo</servlet-name>
  	<display-name>pharmInfo</display-name>
  	<description></description>
  	<servlet-class>usermanagement.web.pharmInfo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PharmacyDashboard</servlet-name>
  	<display-name>PharmacyDashboard</display-name>
  	<description></description>
  	<servlet-class>usermanagement.web.PharmacyDashboard</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>pharmInfo</servlet-name>
  	<url-pattern>/pharmInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PharmacyDashboard</servlet-name>
  	<url-pattern>/PharmacyDashboard</url-pattern>
  </servlet-mapping>
</web-app>
